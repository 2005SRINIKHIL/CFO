# AI Model API Keys
# Get your API keys from the respective providers

# OpenAI API Key (Required for OpenAI and LiteLLM models)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Cohere API Key (Optional - for Cohere Command models)
# Get from: https://dashboard.cohere.ai/api-keys
COHERE_API_KEY=your_cohere_api_key_here

# HuggingFace API Key (Optional - for HuggingFace cloud models)
# Get from: https://huggingface.co/settings/tokens
# Note: Some HuggingFace models work without API key (local models)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Pathway API Key (Optional - for Pathway-specific features)
# Get from: https://pathway.com/
PATHWAY_API_KEY=your_pathway_api_key_here

# FlexPrice API Key (Optional - for additional pricing features)
FLEXPRICE_API_KEY=your_flexprice_api_key_here

# Application Configuration
FLASK_ENV=development
FLASK_DEBUG=false